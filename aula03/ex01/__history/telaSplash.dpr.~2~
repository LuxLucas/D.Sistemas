program telaSplash;

uses
  Vcl.Forms,
  U.tela_splax in 'U.tela_splax.pas' {frm_pincipal},
  U_pg2 in 'U_pg2.pas' {Form1};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(Tfrm_pincipal, frm_pincipal);
  Application.CreateForm(TForm1, Form1);
  Application.Run;
end.
